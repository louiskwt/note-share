<html>
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="/css/w3.css">
        <link rel="stylesheet" href="/css/style.css">
        <title>Note Share</title>
    </head>
    <body>
        <h1>Notes</h1>
        <ul>
            <% files.map(file => { %>
                <li>
                    <a href="<%= file.url %>"><%= file.name %></a>
                </li>
            <% }) %>
        </ul>
    </body>
</html>